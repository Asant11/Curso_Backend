<div>
    <h1>Productos disponibles:</h1>
    {{#if products}}
        {{#each products}}
            <div>
                <p>{{this.title}}</p>
                <p>{{this.id}}</p>
                <p><b>{{this.price}}</b></p>
            </div>
        {{/each }}
    {{else}}
        <h1>No se encontraron productos</h1>
    {{/if}}
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>
